   "source": [
    "%%R -i vmf.res -i datasets -o mm.res.all\n",
    "\n",
    "dnames <- names(datasets)\n",
    "mm.res.all <- list()\n",
    "for(nm in dnames){\n",
    "    mat <- datasets[[nm]]\n",
    "    res.list <- vmf.res[[nm]]\n",
    "    model.names <- sapply(res.list, function(res) paste0(res$E.step,' - ',res$kappa))\n",
    "    partitions <- sapply(res.list, function(res){\n",
    "        pred <- apply(res$model$P, 1, which.max)\n",
    "        return(pred)\n",
    "    })\n",
    "    X = asQualitative(as.data.frame(partitions))\n",
    "    balance <- function(labels){\n",
    "        tab <- table(labels)\n",
    "        return(min(tab)/max(tab))\n",
    "    }\n",
    "    nclass <- length(unique(mat$labels))\n",
    "    print(paste0('---------------------- ', nm, ' ------------------------'))\n",
    "    print(paste0('balance: ', balance(mat$labels)))\n",
    "    print('balance of obtained clusters: ')\n",
    "    print(summary(apply(X,2,balance)))\n",
    "#     head(X)\n",
    "    mm <- mixmodCluster(X, nclass, dataType='qualitative', strategy=mixmodStrategy(nbTry=5))#  criterion=c(\"BIC\", \"ICL\", \"NEC\"))\n",
    "#     str(mm)\n",
    "    mm.res.all[[nm]] <- mm\n",
    "}"
   ]
