    "%%R  -o datasets_descr\n",
    "cluto.dir <- \"./data/cluto/\"\n",
    "cluto_fnames = c('cacmcisi.mat', 'fbis.mat', 'k1b.mat', 'la2.mat', 'ohscal.mat', 'reviews.mat', 'tr12.mat', 'tr41.mat', 'classic.mat', 'hitech.mat', 'la1.mat', 'mm.mat', 're0.mat', 'sports.mat', 'tr23.mat', 'tr45.mat', 'cranmed.mat', 'k1a.mat', 'la12.mat', 'new3.mat', 're1.mat', 'tr11.mat', 'tr31.mat', 'wap.mat')\n",
    "datasets <- lapply(cluto_fnames, function(x) read.mat(paste0(cluto.dir, x)))\n",
    "print(length(datasets))\n",
    "names(datasets) <- cluto_fnames\n",
    "      \n",
    "dnames <- names(datasets)\n",
    "datasets_descr <- sapply(dnames, function(x){\n",
    "    X <- datasets[[x]]$mat\n",
    "    y <- datasets[[x]]$labels\n",
    "    nsamples <- nrow(X)\n",
    "    nfeatures <- ncol(X)\n",
    "    sparsity <- 1-nnzero(X)/length(X)\n",
    "    class.count <- table(y)\n",
    "    nclass <- length(class.count)\n",
    "    balance <- min(class.count)/max(class.count)\n",
    "    return(c(nsamples=nsamples, nfeatures=nfeatures, nclasses=nclass, sparsity=sparsity, balance=balance))\n",
    "}, USE.NAMES=TRUE)\n",
    "datasets_descr <- as.data.frame(t(datasets_descr))\n",
    "class(datasets_descr)"
   ]
