   "source": [
    "%%R -i datasets -o dataset_descr\n",
    "\n",
    "dnames <- names(datasets)\n",
    "dataset_descr <- sapply(dnames, function(x){\n",
    "    X <- datasets[[x]]$mat\n",
    "    y <- datasets[[x]]$labels\n",
    "    nsamples <- nrow(X)\n",
    "    nfeatures <- ncol(X)\n",
    "    sparsity <- 1-nnzero(X)/length(X)\n",
    "    class.count <- table(y)\n",
    "    nclass <- length(class.count)\n",
    "    balance <- min(class.count)/max(class.count)\n",
    "    return(c(nsamples=nsamples, nfeatures=nfeatures, nclasses=nclass, sparsity=sparsity, balance=balance))\n",
    "}, USE.NAMES=TRUE)\n",
    "dataset_descr <- as.data.frame(t(dataset_descr))\n",
    "class(dataset_descr)"
   ]
